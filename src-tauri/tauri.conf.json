{
  "build": {
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "KESAMSATAN STAGING",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "security": {
      "csp": null,
      "dangerousRemoteDomainIpcAccess": [
        {
          "domain": "bapenda-kalselprov.online",
          "windows": [
            "aplikasi"
          ],
          "enableTauriAPI": true
        }
      ]
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.kesamsatan-staging.bapenda-kalselprov.app",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}